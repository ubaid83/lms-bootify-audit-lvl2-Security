		<script>
			
			$(document).ready(function(){
				$(".ui-content a.ui-close").click(function(e){
					e.preventDefault();
					$(this).parents(".ui-173").animate({right:"-270px"});
					$(this).parents(".ui-173").removeClass("open");			
				});
				$(".ui-173 .ui-btn").click(function(){
					var $content = $(this).parent(".ui-173");
					if($content.hasClass("open")){
						$content.animate({right:"-270px"});
						$content.removeClass("open");
					}
					else{
						$content.animate({right:"0px"});
						$content.addClass("open");
					}
				});
			});
		</script>
